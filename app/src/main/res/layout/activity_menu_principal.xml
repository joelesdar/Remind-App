<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Primary"
    tools:context=".MenuPrincipal">

    <Button
        android:id="@+id/button4"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:background="@color/Complementary"
        android:fontFamily="@font/pacifico"
        android:onClick="IngresoTwins"
        android:text="Twins"
        android:textColor="@color/Alert"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.017"
        app:strokeColor="@color/Alert"
        app:strokeWidth="@dimen/buy_button_height" />

    <Button
        android:id="@+id/button7"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:background="@color/Complementary"
        android:fontFamily="@font/pacifico"
        android:textColor="@color/Alert"
        android:textSize="16sp"
        app:cornerRadius="@dimen/margin_large"
        app:icon="@drawable/settings"
        app:iconGravity="textEnd"
        app:iconTint="@color/Alert"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.179"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        app:layout_constraintVertical_bias="0.134"
        app:strokeColor="@color/Alert"
        app:strokeWidth="4dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:background="@color/Complementary"
        android:fontFamily="@font/pacifico"
        android:text="Follow me"
        android:textColor="@color/Alert"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.179"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintVertical_bias="0.064"
        app:strokeColor="@color/Alert"
        app:strokeWidth="4dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:layout_marginTop="4dp"
        android:background="@color/Complementary"
        android:fontFamily="@font/pacifico"
        android:onClick="IngresoProfile"
        android:textColor="@color/Alert"
        android:textSize="16sp"
        app:icon="@drawable/profile"
        app:iconGravity="textEnd"
        app:iconTint="@color/Alert"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toEndOf="@+id/button6"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        app:layout_constraintVertical_bias="0.052"
        app:strokeColor="@color/Alert"
        app:strokeWidth="4dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:background="@color/Complementary"
        android:fontFamily="@font/pacifico"
        android:onClick="IngresoPicture"
        android:text="Picture"
        android:textColor="@color/Alert"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.179"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.017"
        app:strokeColor="@color/Alert"
        app:strokeWidth="4dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="111dp"
        android:layout_height="89dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036"
        app:srcCompat="@drawable/brain" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        app:layout_constraintVertical_bias="0.163"
        app:srcCompat="@drawable/remindtitle" />
</androidx.constraintlayout.widget.ConstraintLayout>