<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/moradoApp"
    tools:context=".Extras">

    <ImageView
        android:id="@+id/brainFondo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:alpha="0.1"
        android:background="@color/purple_700"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/brainverde" />

    <TextView
        android:id="@+id/tituloProfile"
        android:layout_width="413dp"
        android:layout_height="102dp"
        android:fontFamily="@font/pacifico"
        android:lineSpacingExtra="0sp"
        android:text="Bonitos y gorditos Team"
        android:textAlignment="center"
        android:textColor="@color/verdementaApp"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/reyjulien"
        android:layout_width="130dp"
        android:layout_height="132dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/reyjulien"
        app:civ_border_color="#272727"
        app:civ_border_width="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.167"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tituloProfile"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/cabo"
        android:layout_width="130dp"
        android:layout_height="132dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/cabo"
        app:civ_border_color="#272727"
        app:civ_border_width="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.167"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reyjulien"
        app:layout_constraintVertical_bias="0.141"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/kowalski"
        android:layout_width="130dp"
        android:layout_height="132dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/kowalski"
        app:civ_border_color="#272727"
        app:civ_border_width="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toEndOf="@+id/cabo"
        app:layout_constraintTop_toBottomOf="@+id/mort"
        app:layout_constraintVertical_bias="0.141"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/skipper"
        android:layout_width="130dp"
        android:layout_height="132dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/skipper"
        app:civ_border_color="#272727"
        app:civ_border_width="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.167"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cabo"
        app:layout_constraintVertical_bias="0.328"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/rico"
        android:layout_width="130dp"
        android:layout_height="132dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/rico"
        app:civ_border_color="#272727"
        app:civ_border_width="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toEndOf="@+id/skipper"
        app:layout_constraintTop_toBottomOf="@+id/kowalski"
        app:layout_constraintVertical_bias="0.321"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/mort"
        android:layout_width="130dp"
        android:layout_height="132dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/mort"
        app:civ_border_color="#272727"
        app:civ_border_width="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toEndOf="@+id/reyjulien"
        app:layout_constraintTop_toBottomOf="@+id/tituloProfile"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/botonregreso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/verdementaApp"
        android:fontFamily="@font/pacifico"
        android:onClick="Regresar"
        android:text="Volver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/skipper"
        app:layout_constraintVertical_bias="0.978" />

    <TextView
        android:id="@+id/UserNameJulien"
        android:layout_width="130dp"
        android:layout_height="45dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Rey julien"
        android:textAlignment="center"
        android:textColor="#FFC107"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.167"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reyjulien"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/userNameMort"
        android:layout_width="130dp"
        android:layout_height="45dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Mort"
        android:textAlignment="center"
        android:textColor="#FFC107"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toEndOf="@+id/UserNameJulien"
        app:layout_constraintTop_toBottomOf="@+id/reyjulien"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/userNameCabo"
        android:layout_width="130dp"
        android:layout_height="45dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Cabo"
        android:textAlignment="center"
        android:textColor="#FFC107"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.167"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cabo"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/userNameKowalski"
        android:layout_width="130dp"
        android:layout_height="45dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="kowalski"
        android:textAlignment="center"
        android:textColor="#FFC107"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toEndOf="@+id/userNameCabo"
        app:layout_constraintTop_toBottomOf="@+id/kowalski"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/userNameSkipper"
        android:layout_width="130dp"
        android:layout_height="45dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Skipper"
        android:textAlignment="center"
        android:textColor="#FFC107"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.167"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/skipper"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/userNameRico"
        android:layout_width="130dp"
        android:layout_height="45dp"
        android:fontFamily="@font/quicksand_medium"
        android:text="Rico"
        android:textAlignment="center"
        android:textColor="#FFC107"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toEndOf="@+id/userNameSkipper"
        app:layout_constraintTop_toBottomOf="@+id/rico"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>