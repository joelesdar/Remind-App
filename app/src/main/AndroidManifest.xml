<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.remind_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/brain"
        android:label="@string/app_name"
        android:roundIcon="@drawable/brain"
        android:supportsRtl="true"
        android:theme="@style/Theme.RemindApp"
        tools:targetApi="31">
        <activity
            android:name=".Extras"
            android:exported="true" />
        <activity
            android:name=".followMe"
            android:exported="true" />
        <activity
            android:name=".followMeGameScreen"
            android:exported="true" />
        <activity
            android:name=".Profile"
            android:exported="true" />
        <activity
            android:name=".picture.videoGuia"
            android:exported="true" />
        <activity
            android:name=".twins.videoGuia"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".Juego" />
        <activity
            android:name=".picture.pictureGameScreen"
            android:exported="true" />
        <activity
            android:name=".Picture"
            android:exported="true" />
        <activity
            android:name=".MenuPrincipal"
            android:exported="true">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".RemindMain"
            android:exported="true"
            android:label="@string/remind_app"
            android:theme="@style/Theme.RemindApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- This element is required to enable Google Pay in your app. -->
        <meta-data
            android:name="com.google.android.gms.wallet.api.enabled"
            android:value="true" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>